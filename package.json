    "@types/node": "^22.15.3",
    "@zerodevx/svelte-toast": "^0.9.6",
    "daisyui": "^5.0.28",
    "eslint": "^9.25.1",
    "eslint-config-prettier": "^10.1.2",
    "eslint-plugin-svelte": "^3.5.1",
    "globals": "^16.0.0",
    "husky": "^9.1.7",
    "lint-staged": "^15.5.1",
